Basic:

my_dir = "Root Directory Here"
   print("**********Start Print**********")
   for root_dir_path, sub_dirs, files in os.walk(my_dir):
     print("Root Directory Path: ", root_dir_path)
     print("Sub Directories: ", sub_dirs)
     print("Files", files)
     print('*' * 25)
   print("**********End Print**********")
   


Only Files:

 my_dir = "Root Directory Here"
   print("**********Start Print**********")
   for dir, sub, files in os.walk(my_dir):
    print("Print Dir: ", dir)
    sub = [n for n in sub]
    contents = sub + files
    contents.sort()
    
    for f in contents:
       f = os.path.join(dir,f)
       if os.path.isfile(f):
         print('\tJust The Files', f)
    print()
    print("**********End Print**********")



Only Subfolders:

my_dir = "Root Directory Here"
   print("**********Start Print**********")
   for dir, sub, files in os.walk(my_dir):
    print("Print Dir: ", dir)
    sub = [n for n in sub]
    sub.sort()
    
    for f in sub:
       print('\tJust The Subs', f)
    print()
    print("**********End Print**********")
    
    
    
File Count:

my_dir = "C:\hgrepos\cadiq_itar\cadiq\config\config_nse"
   count_file = 0
   
   print("**********Start Print**********")
   for dir, sub, files in os.walk(my_dir):
    sub = [n for n in sub]
    content = sub + files
   for f in content:
      count_file += 1
      print("Count:{} File Name: {}".format(count_file, f))
    
   print("Final count is: ", count_file)
   print("**********End Print**********")
